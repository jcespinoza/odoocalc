<odoo>
    <data>
        <template id="assets_frontend" inherit_id="website.assets_frontend" name="Calculator Module">
            <xpath expr="." position="inside">
                <base href="/" />
                <link rel="stylesheet" href="/odoocalc/static/src/css/styles.css"/>
            </xpath>
        </template>
        <template id="index" name="Calculator Template">
            <t t-call="website.layout">
                <app-root></app-root>
                <script src="/odoocalc/static/src/js/runtime-es2015.js" type="text/javascript"></script>
                <script src="/odoocalc/static/src/js/polyfills-es2015.js" type="text/javascript"></script>
                <script src="/odoocalc/static/src/js/runtime-es5.js" type="text/javascript" nomodule="nomodule"></script>
                <script src="/odoocalc/static/src/js/polyfills-es5.js" type="text/javascript" nomodule="nomodule"></script>
                <script src="/odoocalc/static/src/js/main-es2015.js" type="text/javascript"></script>
                <script src="/odoocalc/static/src/js/main-es5.js" type="text/javascript" nomodule="nomodule"></script>
            </t>
        </template>
    </data>
</odoo>